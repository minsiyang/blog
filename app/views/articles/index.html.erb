<h1>Listing Articles</h1>
 
<button>
<%= link_to 'New article', new_article_path %> 
</button>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th></th>
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
      <td><button><%= link_to 'Show', article_path(article) %></button></td>
      <td><button><%= link_to 'Edit', edit_article_path(article) %></button></td>
    </tr>
  <% end %>
</table>
